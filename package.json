{
  "name": "hull-slack",
  "description": "Send Notification of User events and segments to Slack",
  "version": "0.0.3",
  "homepage": "https://github.com/hull-ships/hull-slack",
  "license": "MIT",
  "main": "bin/start",
  "author": {
    "name": "Hull",
    "email": "contact@hull.io",
    "url": "https://github.com/hull"
  },
  "engines": {
    "node": "6.x",
    "npm": "3.x",
    "yarn": "0.27.5"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hull-ships/hull-slack"
  },
  "bugs": {
    "url": "https://github.com/hull-ships/hull-slack/issues"
  },
  "scripts": {
    "build": "npm run clean && npm run build:server && npm run build:client",
    "build:client": "NODE_ENV=production webpack --config ./webpack.config.js --progress --profile --colors",
    "build:server": "babel server -d lib",
    "start": "node ./lib",
    "start:dev": "NODE_ENV=development LOG_LEVEL=debug nodemon -V -w server -x babel-node -- server",
    "ngrok": "ngrok http 8082 --region eu --subdomain slack",
    "test": "npm run test:lint && npm run test:modules",
    "test:lint": "eslint server",
    "test:modules": "npm outdated --depth=0",
    "clean": "rimraf dist; rimraf lib",
    "update": "updtr",
    "watch": "watch 'clear && npm run test:lint -s && npm run test:units -s' server src",
    "postinstall": "npm run build"
  },
  "keywords": [
    "hull",
    "ship",
    "hull-ship",
    "webhooks",
    "integration",
    "user",
    "sync",
    "slack"
  ],
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.1.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^7.4.0",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^1.3.0",
    "updtr": "^0.2.3",
    "watch": "^1.0.1"
  },
  "dependencies": {
    "antd": "^2.13.4",
    "axios": "^0.16.2",
    "babel-cli": "^6.10.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.9.0",
    "body-parser": "^1.15.2",
    "botkit": "^0.4.9",
    "bson-objectid": "^1.1.4",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "domready": "^1.0.8",
    "dotenv": "^4.0.0",
    "draft-js": "^0.10.3",
    "draft-js-mention-plugin": "^2.0.0-rc8",
    "draft-js-plugins-editor": "^2.0.0-rc8",
    "ejs": "^2.5.5",
    "express": "^4.14.0",
    "flat": "^4.0.0",
    "hull": "0.11.8",
    "immutable": "^3.8.2",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "moment": "^2.17.1",
    "node-slack": "0.0.7",
    "passport": "^0.3.2",
    "passport-slack": "0.0.7",
    "react": "^15.6.2",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^15.6.2",
    "react-select": "^1.0.0-rc.10",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "svg-inline-loader": "^0.6.0",
    "svg-inline-react": "^1.0.1",
    "textarea-caret": "^3.0.2",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.12.0",
    "winston-logstash": "^0.3.0"
  }
}
